# 容器设置 最好别改
TZ="Asia/Shanghai"  
ALWAYS_UPDATE_ON_START=true
MULTITHREAD_ENABLED=true
COMMUNITY_SERVER=true
BACKUP_ENABLED=false
STEAMCMD_VALIDATE_FILES=true
SERVER_SETTINGS_MODE=auto

# 服务器设置
#服务器帧率
NETSERVERMAXTICKRATE=60

#游戏难度 None Easy Normal Hard 即使使用Difficult或是Easy也不影响下列调整的参数
DIFFICULTY=None

#白天速度 (0.1 - 5)
DAYTIME_SPEEDRATE=1.000000

#夜晚速度 (0.1 - 5)
NIGHTTIME_SPEEDRATE=1.000000

#经验倍率
EXP_RATE=1.000000

#帕鲁抓捕机率 (0.5 - 2) PS: 这个参数调整非常敏感请谨慎调整。困难模式为0.8，简单模式为2
PAL_CAPTURE_RATE=1.000000

#帕鲁的重生数量 若调整为2，王固定变2只，野生帕鲁群2~4只 PS: 超过3会经常导致帕鲁重叠或掉出地图，请谨慎调整。
PAL_SPAWN_NUM_RATE=1.000000

#帕鲁造成的伤害倍率
PAL_DAMAGE_RATE_ATTACK=1.000000

#帕鲁受到的伤害倍率
PAL_DAMAGE_RATE_DEFENSE=1.000000

#玩家造成的伤害倍率 (0.1 - 5)
PLAYER_DAMAGE_RATE_ATTACK=1.000000

#玩家受到的伤害倍率 (0.1 - 5)
PLAYER_DAMAGE_RATE_DEFENSE=1.000000

#玩家的饱足感下降速度 (0.1 - 5)
PLAYER_STOMACH_DECREASE_RATE=1.000000

#玩家的耐力条下降速度 (0.1 - 5)
PLAYER_STAMINA_DECREACE_RATE=1.000000

#玩家的自动生命回复速率
PLAYER_AUTO_HP_REGENE_RATE=1.000000

#玩家睡眠中的生命回复倍率
PLAYER_AUTO_HP_REGENE_RATE_IN_SLEEP=1.000000

#帕鲁的饱足感下降速度 (0.1 - 5)
PAL_STOMACH_DECREACE_RATE=1.000000

#帕鲁的耐力条下降速度 (0.1 - 5)
PAL_STAMINA_DECREACE_RATE=1.000000

#帕鲁的自动生命回复速率
PAL_AUTO_HP_REGENE_RATE=1.000000

#帕鲁睡眠中的生命回复倍率 (在帕鲁箱中)
PAL_AUTO_HP_REGENE_RATE_IN_SLEEP=1.000000

#建筑受伤害倍率 (0.5 - 3)
BUILD_OBJECT_DAMAGE_RATE=1.000000

#建筑劣化速率 (0 - 10)
BUILD_OBJECT_DETERIORATION_DAMAGE_RATE=1.000000

#采集资源倍率 例 : 拿石稿敲小帕鲁矿取得1个帕鲁碎片默认要6下平均19点伤害，若调为3则要敲2下平均19点伤害。
COLLECTION_DROP_RATE=1.000000

#采集资源生命倍率 (0.5 - 3) PS: 经敲小帕鲁矿，测试不生效
COLLECTION_OBJECT_HP_RATE=1.000000

#采集资源重生间隔 (0；0.5 - 3) PS: 树木大矿石为1小时，若为0立即重生
COLLECTION_OBJECT_RESPAWN_SPEED_RATE=1.000000

#敌人掉落倍率
ENEMY_DROP_ITEM_RATE=1.000000

#死亡惩罚 None: 全部不掉落 Item: 仅掉落道具(不含帕鲁及身上穿的装备武器) ItemAndEquipment: 除了帕鲁其他全掉 All: 帕鲁、装备、道具全部掉落(不包含无法掉落的帕鲁专属装备，如马鞍)
DEATH_PENALTY=All

#False [推荐用于PVP] 玩家对玩家伤害 (true/false)
ENABLE_PLAYER_TO_PLAYER_DAMAGE=false

#[推荐用于PVP] 玩家对自己帕鲁跟同公会玩家的伤害 (True/False)
ENABLE_FRIENDLY_FIRE=false

#袭击事件关闭开启 (True/False)
ENABLE_INVADER_ENEMY=true

#神秘的UNKO事件关闭开启 (True/False)
ACTIVE_UNKO=false

#手把辅助瞄准 (True/False)
ENABLE_AIM_ASSIST_PAD=true

#键鼠辅助瞄准 (True/False)
ENABLE_AIM_ASSIST_KEYBOARD=false

#世界内的掉落物上限
DROP_ITEM_MAX_NUM=3000

#神秘UNKO掉落物上限
DROP_ITEM_MAX_NUM_UNKO=100

#基地最大数量
BASE_CAMP_MAX_NUM=128

#基地内工作帕鲁的最大数量
BASE_CAMP_WORKER_MAXNUM=15

#掉落物品的保留时间 PS: 建议设为0.5以保证掉落物不会太多造成卡顿
DROP_ITEM_ALIVE_MAX_HOURS=1.000000

#自动帮玩家退出公会 (True/False)
AUTO_RESET_GUILD_NO_ONLINE_PLAYERS=false

#72(h) 自动帮玩家退出公会的时间
AUTO_RESET_GUILD_TIME_NO_ONLINE_PLAYERS=72.000000

#公会人数上限
GUILD_PLAYER_MAX_NUM=20

#1(h) 蛋的孵化时间
PAL_EGG_DEFAULT_HATCHING_TIME=72.000000

#工作效率
WORK_SPEED_RATE=1.000000

#未知作用 (True/False)
IS_MULTIPLAY=false

#[推荐用于PVP] 多人游戏与PVP模式 (True/False)
IS_PVP=false

#[推荐用于PVP] 是否捡取其他公会的死亡掉落物 (True/False)
CAN_PICKUP_OTHER_GUILD_DEATH_PENALTY_DROP=false

#[推荐用于PVP] 登录惩罚 (True/False) [未知作用]
ENABLE_NON_LOGIN_PENALTY=true

#是否开启快速旅行 (True/False)
ENABLE_FAST_TRAVEL=true

#是否创角后选择出生点 (True/False) PS: 若为关则出生在初始台地，死亡后依旧可以选择其他出生点
IS_START_LOCATION_SELECT_BY_MAP=true

#玩家登出是否留在原地（不消失） (True/False)
EXIST_PLAYER_AFTER_LOGOUT=false

# [推荐用于PVP] 是否受到其他公会伤害 (True/False)
ENABLE_DEFENSE_OTHER_GUILD_PLAYER=false

#合作玩家人数
COOP_PLAYER_MAX_NUM=4

#服务器最大人数
MAX_PLAYERS=32

#服务器名称
SERVER_NAME="PalworldServer"

#服务器简介
SERVER_DESCRIPTION="Palworld-Dedicated-Server running in Docker"

#服务器管理员密码
ADMIN_PASSWORD=adminpasswd

#服务器密码 默认空 不需要密码进服
SERVER_PASSWORD=

#主机端口
SERVER_PORT=8211

#主机IP 最好为空
PUBLIC_IP=

#RCON 使用或关闭 必须开启，脚本需要使用RCON进行交互
RCON_ENABLED=true

#RCON 端口
RCON_PORT=25575

#不需要修改
REGION=

#是否使用授权 (作用未知)/nPS: 应该是向官方验证的服务
USEAUTH=true

#封锁名单 (目前来自官方) 没啥用
BAN_LIST_URL=https://api.palworldgame.com/api/banlist.txt

#脚本启动项 不会的千万别改
STARTUP=./PalServer.sh port=${SERVER_PORT}